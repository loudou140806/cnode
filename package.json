{
  "name": "cnode",
  "version": "1.0.0",
  "description": "cnode based on react",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf cnode",
    "dev:dll": "webpack --config webpack.dll.config.js --progress",
    "build:dll": "webpack --config webpack.dll.config.js -p --progress",
    "dev": "npm run dev:dll && node server.js",
    "build:webpack": "webpack --config webpack.config.js --progress --color -p",
    "build": "npm run clean && npm run build:dll && npm run build:webpack",
    "deploy": "gh-pages -d cnode"
  },
